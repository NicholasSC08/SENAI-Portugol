    cadeia verificacaoDeNomeDoUsuario 
    cadeia nomeDeUsuario = "admin"
    inteiro verificacaoDeSenhaDeUsuario
    inteiro senhaDeUsuario = 123
escreva("Bem vindo de volta, ",nomeDeUsuario,"!")
    escreva("\n\nDigite a o seu nome de usuário: ")
    leia(verificacaoDeNomeDoUsuario)
escreva("\nDigite sua senha: ")
leia(verificacaoDeSenhaDeUsuario)
se(verificacaoDeNomeDoUsuario != nomeDeUsuario e verificacaoDeSenhaDeUsuario != senhaDeUsuario){
 escreva("\nUsuário e senha incorretos!")
} senao se(nomeDeUsuario == verificacaoDeNomeDoUsuario e verificacaoDeSenhaDeUsuario == senhaDeUsuario){
  escreva("\nLogin efetuado!")
} senao se(verificacaoDeNomeDoUsuario != nomeDeUsuario){
  escreva("\nUsuário incorreto!")
